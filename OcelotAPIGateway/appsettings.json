{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "Application": {
      "Name": "OcelotAPIGateway"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl":"http://EurekaRegistry:8761/eureka/",      
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true,
      "ShouldFetchRegistry": true
    },
    "Instance": {
      "NonSecurePort": 8080,
      "HostName": "OcelotAPIGateway",
      "InstanceId": "OcelotAPIGatewayPROD:8080"
    }
  },
  "AllowedHosts": "*"
}